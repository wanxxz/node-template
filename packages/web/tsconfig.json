{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "~/*": ["src/*"]
    },
    "target": "esnext",
    "types": ["vite/client"]
  }
}
